<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.java.mvc.javamvc.Controllers.MemberMenuController">
   <children>
      <Text fill="#747d8c" layoutX="196.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Member Menu">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Text>
      <Button layoutX="227.0" layoutY="106.0" mnemonicParsing="false" onAction="#AddMember" prefHeight="37.0" prefWidth="146.0" style="-fx-background-color: #f1c40f;" text="Add Member">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="227.0" layoutY="164.0" mnemonicParsing="false" onAction="#UpdateMember" prefHeight="37.0" prefWidth="146.0" style="-fx-background-color: #f1c40f;" text="Update Member">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="227.0" layoutY="218.0" mnemonicParsing="false" onAction="#DeleteMember" prefHeight="37.0" prefWidth="146.0" style="-fx-background-color: #f1c40f;" text="Delete Member">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="227.0" layoutY="272.0" mnemonicParsing="false" onAction="#LoadAllMember" prefHeight="37.0" prefWidth="146.0" style="-fx-background-color: #f1c40f;" text="Load All Member">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <ImageView fitHeight="25.0" fitWidth="27.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#Back" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/Back.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
